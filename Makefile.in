@ONLY_RPMRC_TRUE@	echo "not installing per-arch macros which are already in rpmrc and standard rpm per-arch macros"
@PLATFORMMACROS_TRUE@	for i in $(RPMALLARCH); do \
@PLATFORMMACROS_TRUE@	  $(mkinstalldirs) $(DESTDIR)$(RPMVENDORDIR)/platform/$${i}-$(RPMOS); \
@PLATFORMMACROS_TRUE@	  $(install_sh_DATA) $${i}.macros $(DESTDIR)$(RPMVENDORDIR)/platform/$${i}-$(RPMOS)/macros; \
@PLATFORMMACROS_TRUE@	done
@RPMPLATFORM_TRUE@	for i in $(pkg_gconfig); do \
@RPMPLATFORM_TRUE@		$(install_sh_DATA) $${i} $(DESTDIR)$(RPMSYSCONFDIR)/$${i}; \
@RPMPLATFORM_TRUE@	done
@RPMPLATFORM_TRUE@	$(install_sh_SCRIPT) rpmgenplatform $(DESTDIR)$(bindir)/rpmgenplatform

.PHONY: ChangeLog

ChangeLog:
	@if [ -e ".svn" ]; then \
	    $(MAKE) ChangeLog-svn; \
	elif [ -e ".git" ]; then \
	    $(MAKE) ChangeLog-git; \
	else \
	    echo "Unknown SCM (not SVN nor GIT)";\
	    exit 1; \
	fi;

ChangeLog-svn:
	LC_ALL=C svn2cl
	rm -f *.bak

ChangeLog-git:
	@git2cl >ChangeLog

